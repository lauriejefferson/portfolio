---js
const eleventyNavigation = {
	key: "Home",
	order: 1
};
---
<section class='hero'>
	<h1 class='hero-title'>I'm a Frontend Web Developer based in Washington, DC.</h1>
	<a class="primary-cta" href='/'>Get In Touch</a>
</section>
<section class='hire'>
		<h1 class='hire-title'>Why Hire Me?</h1>
		<p class='hire-text'>Lorem ipsum dolor sit amet consectetur adipisicing elit. Porro, non odio? Consequatur eveniet fuga doloremque veritatis ab tempore a suscipit!</p>
		<ul class='skills-list'>
			<li class='skill-list-item'>
				<div class='skill-name'>
				<p>HTML</p>
				<p>90%</p>
				</div>
				<div class="skill-indicator"></div>
			</li>
			<li class='skill-list-item'>
				<div class='skill-name'>
					<p>CSS</p>
					<p>90%</p>
				</div>
				<div class="skill-indicator"></div>
			</li>
			<li class='skill-list-item'>
					<div class='skill-name'>
				   <p>JS</p>
					 <p>90%</p>
					</div>
					<div class="skill-indicator"></div>
			</li>
			<li class='skill-list-item'>
					<div class='skill-name'>
				   <p>11ty</p>
					 <p>80%</p>
					</div>
					<div class="skill-indicator"></div>
			</li>
			<li class='skill-list-item'>
				<div class='skill-name'>
					<p>VUE</p>
					<p>90%</p>
				</div>
				<div class="skill-indicator"></div>
			</li>
			<li class='skill-list-item'>
				<div class='skill-name'>
				   <p>Astro.js</p>
					 <p>80%</p>
				</div>
				<div class="skill-indicator"></div>
			</li>
		</ul>

</section>
<section class='projects'>
		<h1 class='projects-title'>Featured Projects</h1>
		<ul class='projects-list'>
			{% for item in github %}
				{% if item.featured == true %}
				<li class='project-list-item'>
					<div class='project'>
						<h2 class='project-title'>{{ item.name }}</h2>
						<div class='project-links'>
							<a href="{{ item.repository }}" class='project-github-link' ><box-icon type='logo' name='github' color='#002626'></box-icon></a>
							<a href="{{ item.site }}" class='project-link'><box-icon name='link-external' color='#002626' ></box-icon></a>
						</div>
					</div>
					<img src="{{ item.image }}" alt="frontend-mentor" height="300">
				</li>
				{% endif %}
		{% endfor %}
		</ul>
</section>
<section class='contact'>
	<h1 class='contact-title'>Let's Work Together</h1>
	<p class='contact-text'>Have a project in mind? Want to discuss opportunities with your organization? Let's discuss.</p>
	<a class='secondary-cta' href='/'>Contact Me</a>
</section>

